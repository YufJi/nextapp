{
  "presets": [
    ["next/babel"]
  ],
  "plugins": [
    ["transform-assets-import-to-string", { "baseDir": "static", "baseUri": "/" }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-proposal-export-default-from"]
  ]
}
